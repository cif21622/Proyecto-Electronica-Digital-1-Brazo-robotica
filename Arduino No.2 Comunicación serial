// Arduino Esclavo/Receiver


#include <Servo.h>
#include <EEPROM.h>

Servo servo1;
Servo servo2;
Servo servo3;
Servo servo4;

int ultimaPosicion[4] = {0, 0, 0, 0};
bool guardarPosiciones = false;

void setup() {
  

  
  Serial.begin(9600);

  servo1.attach(8);
  servo2.attach(9);
  servo3.attach(10);
  servo4.attach(11);
}
